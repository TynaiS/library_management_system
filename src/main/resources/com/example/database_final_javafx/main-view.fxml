<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.Region?>
<?import javafx.scene.layout.StackPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<BorderPane prefHeight="600" prefWidth="750.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.example.database_final_javafx.MainController">
    <VBox />
    <bottom>
        <StackPane fx:id="contentPane" minHeight="300.0" minWidth="300.0" prefHeight="561.0" prefWidth="700.0" BorderPane.alignment="CENTER">
        </StackPane>
    </bottom>
    <top>
        <HBox alignment="CENTER" spacing="20.0" BorderPane.alignment="TOP_CENTER">
            <Label fx:id="labelBookStore" text="Book Store">
            <font>
               <Font name="System Bold" size="24.0" />
            </font>
            <HBox.margin>
               <Insets left="10.0" />
            </HBox.margin></Label>
            <Region HBox.hgrow="ALWAYS" />
            <Button fx:id="logoutButton" mnemonicParsing="false" onAction="#handleLogout" text="Logout" visible="false">
            <HBox.margin>
               <Insets right="10.0" />
            </HBox.margin></Button>
        </HBox>
    </top>
</BorderPane>
